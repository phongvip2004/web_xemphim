<div class="search-form">
    <form method="get" action="/news">
  <div class="form-group">
    <label for="search-input">Từ khóa</label>
    <input type="text" name="fullname" placeholder="Họ và tên" class="form-control">
      <input type="text" name="age" placeholder="Tuổi" class="form-control">
    <input type="text"  name="q" class="form-control" id="search-input" placeholder="Nhập từ khóa">
  
  </div>
  
  <button type="submit" class="btn btn-primary">Search</button>
</form>
</div>



